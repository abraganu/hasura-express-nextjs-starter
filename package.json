{
  "name": "clinica",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "nx build",
    "codegen:gql": "graphql-codegen --config codegen.yml",
    "codegen:prisma": "prisma db pull --force && prisma generate",
    "dev:web": "nx serve",
    "dev:api": "npm-run-all start:hasura-engine -p -r start:hasura-console watch start:api-admin",
    "start:hasura-engine": "docker-compose up -d",
    "start:hasura-console": "wait-on http-get://127.0.0.1:8080/v1/version/ && hasura console --no-browser",
    "start:api-admin": "wait-on http-get://127.0.0.1:8080/v1/version/ && nx serve api-admin",
    "start:web-admin": "nx serve web-admin",
    "test": "nx test",
    "watch": "wait-on http-get://127.0.0.1:8080/v1/version/ && yarn npm-watch"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@emotion/react": "11.10.5",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.5",
    "@mantine/core": "^5.9.5",
    "@mantine/hooks": "^5.9.5",
    "@mantine/next": "^5.9.5",
    "@nrwl/next": "15.3.3",
    "@prisma/client": "^4.8.0",
    "core-js": "^3.6.5",
    "cross-fetch": "^3.1.5",
    "deepmerge": "^4.2.2",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "next": "13.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "7.12.13",
    "@emotion/babel-plugin": "11.10.5",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/near-operation-file-preset": "^2.4.4",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@nrwl/cli": "15.3.3",
    "@nrwl/cypress": "15.3.3",
    "@nrwl/eslint-plugin-nx": "15.3.3",
    "@nrwl/express": "15.3.3",
    "@nrwl/jest": "15.3.3",
    "@nrwl/linter": "15.3.3",
    "@nrwl/node": "15.3.3",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/react": "15.3.3",
    "@nrwl/storybook": "15.3.3",
    "@nrwl/web": "15.3.3",
    "@nrwl/workspace": "15.3.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/builder-webpack5": "~6.5.9",
    "@storybook/core-server": "~6.5.9",
    "@storybook/manager-webpack5": "~6.5.9",
    "@storybook/react": "~6.5.9",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/react": "13.4.0",
    "@types/express": "4.17.14",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "babel-jest": "28.1.3",
    "babel-loader": "8.1.0",
    "css-loader": "^6.4.0",
    "cypress": "^11.0.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "hasura-cli": "^2.15.1",
    "jest": "28.1.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "nx": "15.3.3",
    "prettier": "^2.6.2",
    "prisma": "^4.8.0",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "18.2.0",
    "style-loader": "^3.3.0",
    "stylus": "^0.55.0",
    "stylus-loader": "^7.1.0",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "url-loader": "^4.1.1",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-merge": "^5.8.0"
  },
  "watch": {
    "codegen:prisma": {
      "patterns": [
        "migrations",
        "metadata"
      ],
      "extensions": "sql,yml,yaml",
      "quiet": false
    },
    "codegen:gql": {
      "patterns": [
        "migrations",
        "metadata",
        "apps",
        "libs"
      ],
      "extensions": "sql,yml,yaml,graphql,gql",
      "quiet": false
    }
  }
}
